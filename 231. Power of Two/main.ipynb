      "source": [
        "class Solution:\n",
        "  def isPowerOfTwo(self, n: int) -> bool:\n",
        "\n",
        "    while n > 1 and n % 2 == 0:\n",
        "      n = n // 2\n",
        "\n",
        "    return n == 1"
      ]
    },
